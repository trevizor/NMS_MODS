      <DrawQuad material="materials/PostProcess.material.mbin" context="GUASS_9_ADD" compute="async_next" />
      <UnbindBuffers />
      <EndTarget flushCB="true" flushDB="false" />
    </Stage>


    <Stage id="LensFlareResolve">      
